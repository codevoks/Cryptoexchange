{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "dotenv -e .env -- turbo run build",
    "dev": "dotenv -e .env -- turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "clean": "rm -rf node_modules .turbo apps/web/.next",
    "db:migrate": "dotenv -e .env -- npx prisma migrate dev --schema=packages/db/prisma/schema.prisma",
    "db:studio": "dotenv -e .env -- npx prisma studio --schema=packages/db/prisma/schema.prisma"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.10",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^6.11.0",
    "@types/redis": "^4.0.10",
    "jose": "^6.1.0",
    "lightweight-charts": "^5.0.8",
    "redis": "^5.5.6"
  }
}
